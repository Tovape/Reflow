<!DOCTYPE HTML>
<html lang="en-US">
	<link rel="icon" href="/logo/logo3.png" type="image/gif" sizes="16x16">
	<link rel="stylesheet" type="text/css" href="global.css">
	<link rel="stylesheet" type="text/css" href="editor.css">
	<link rel="stylesheet" type="text/css" href="global-responsive.css">
	<link rel="stylesheet" type="text/css" href="editor-responsive.css">
	<link rel="preconnect" href="https://fonts.googleapis.com">
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
	<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
	<!-- Fonts -->
	<link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300&display=swap" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css2?family=Montserrat&display=swap" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css2?family=Raleway&display=swap" rel="stylesheet">
<head>
	<title>Reflow</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta charset="UTF-8"/>
	<script src="https://kit.fontawesome.com/6308dbf55d.js" crossorigin="anonymous"></script>
	<script src="https://cdn.websitepolicies.io/lib/cookieconsent/1.0.3/cookieconsent.min.js" defer></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.js"></script>
	<script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/quicklink/2.2.0/quicklink.umd.js"></script>
	<script type="text/javascript" src="editor-canvas.js"></script>
	<script type="text/javascript" src="global-javascript.js"></script>
	<script type="text/javascript" src="global-language.js"></script>
	<script type="text/javascript" src="editor-language.js"></script>
	<script type="text/javascript" src="editor-javascript.js"></script>
	<script type="text/javascript" src="three.min.js"></script>
	<script type="text/javascript" src="blueprint3d.js"></script>
</head>

<body>

<div class="editor-aligner">
	<div class="editor-menu" id="editor-menu">
		<div class="editor-menu-logo" id="editor-menu-logo">
			<div class="editor-menu-bars" id="editor-menu-bars">
				<a href="javascript:void(0);" onclick="menucollapse()"/>
					<svg version="1.1" version="1.1" viewBox="0 0 512 512" xml:space="preserve" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g data-name="1" id="_1"><path d="M441.13,166.52h-372a15,15,0,1,1,0-30h372a15,15,0,0,1,0,30Z"/><path d="M441.13,279.72h-372a15,15,0,1,1,0-30h372a15,15,0,0,1,0,30Z"/><path d="M441.13,392.92h-372a15,15,0,1,1,0-30h372a15,15,0,0,1,0,30Z"/></g></svg>
				</a>
			</div>
			<a href="/" id="editor-menu-logo-a">
				<img data-lazy="/logo/logo.png">
			</a>
		</div>
		<div class="editor-menu-user">
			<div class="editor-profile-avatar" id="editor-profile-avatar"></div>
			<div>
				<p id="user" style="display: none"><%= user_data[Object.keys(user_data)[0]] %></p>
				<p><%= user_data[Object.keys(user_data)[1]] %></p>
				<p><%= user_data[Object.keys(user_data)[2]] %></p>
			</div>
		</div>
		<div class="editor-menu-scrollable">
			<div class="editor-menu-section">
				<p key="editor-name">Name</p>
			</div>
			<div class="editor-menu-input">
				<input type="text" id="request_title" name="request_title" placeholder="Name" value="" required>
			</div>
			<div class="editor-menu-section">
				<p key="editor-description">Description</p>
			</div>
			<div class="editor-menu-input">
				<input type="text" id="request_description" name="request_description" placeholder="Description" value="" required>
			</div>
			<div class="editor-menu-button">
				<button key="editor-save" onclick="saveRequest()">Save</button>
			</div>
			<div class="editor-menu-section">
				<p key="editor-house">House</p>
			</div>
			<div class="editor-menu-each">
				<div class="editor-menu-view editor-menu-button">
					<p class="active" id="editor-menu-view-2d">2D</p>
					<p class="" id="editor-menu-view-3d">3D</p>
				</div>
			</div>
			<div class="editor-menu-section">
				<p key="editor-inventory">Inventory</p>
			</div>
			<div class="editor-menu-button">
				<p key="editor-open" id="editor-inventory-open">Open</p>
			</div>
			<div class="editor-menu-section">
				<p key="editor-material">Materials</p>
			</div>
			<div class="editor-menu-button">
				<p key="editor-open" id="editor-material-open">Open</p>
			</div>
			<div class="editor-menu-section">
				<p key="editor-settings">Settings</p>
			</div>
			<div class="editor-menu-button editor-menu-delete">
				<p key="editor-delete" id="editor-delete">Delete</p>
			</div>
			<div class="editor-menu-each editor-menu-each-nopadding">
				<div class="editor-menu-each-setting">
					<label class="pure-material-switch">
						<input type="checkbox">
						<span key="editor-darkmode">Dark Mode</span>
					</label>
				</div>
			</div>
			<div class="editor-menu-each editor-menu-each-nopadding">
				<div class="editor-menu-each-button">
					<p key="editor-language">Language</p>
					<i class="fa-solid fa-angle-up"></i>
				</div>
				<div class="editor-menu-each-content editor-menu-each-rooms">
					<div class="language-dropdown-each">
						<a href="#" class="language" id="es" key="spanish">Español</a>
					</div>
					<div class="language-dropdown-each">
						<a href="#" class="language" id="ca" key="catalan">Català</a>
					</div>
					<div class="language-dropdown-each">
						<a href="#" class="language" id="en" key="english">English</a>
					</div>
				</div>
			</div>
			<div class="editor-menu-each editor-menu-each-nopadding">
				<div class="editor-menu-each-button">
					<p key="editor-measurement">Measurement</p>
					<i class="fa-solid fa-angle-up"></i>
				</div>
				<div class="editor-menu-each-content editor-menu-each-rooms">
					<div class="language-dropdown-each measurement-active">
						<a href="#" class="measurement" id="meters" key="editor-meters">Meters</a>
					</div>
					<div class="language-dropdown-each">
						<a href="#" class="measurement" id="inches" key="editor-inches">Feet & Inches</a>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="editor-canvas" id="editor-canvas">
		<div id="floorplanner" style="height: 1041px;">
			<canvas id="floorplanner-canvas"></canvas>
			<div id="floorplanner-controls">
				<button id="move">
					<i class="fa-solid fa-arrow-pointer editor-menu-tools-each"></i>
				</button>
				<button id="draw">
					<i class="fa-solid fa-pen editor-menu-tools-each"></i>
				</button>
				<button id="delete">
					<i class="fa-solid fa-eraser editor-menu-tools-each"></i>
				</button>
			</div>
		</div>
		<div id="viewer" style="display:none">
			<!-- Here Goes main-controls -->
			<div id="camera-controls">
				<div id="zoomin">
					<i class="fa-solid fa-plus"></i>
				</div>
				<div id="zoomdef">
					<i class="fa-solid fa-magnifying-glass"></i>
				</div>
				<div id="zoomout">
					<i class="fa-solid fa-minus"></i>
				</div>
			</div>
			<div id="loading-modal">
				<svg version="1.1" id="L9" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
				viewBox="0 0 100 100" enable-background="new 0 0 0 0" xml:space="preserve">
				<path fill="#fff" d="M73,50c0-12.7-10.3-23-23-23S27,37.3,27,50 M30.9,50c0-10.5,8.5-19.1,19.1-19.1S69.1,39.5,69.1,50">
				<animateTransform 
				attributeName="transform" 
				attributeType="XML" 
				type="rotate"
				dur="1s" 
				from="0 50 50"
				to="360 50 50" 
				repeatCount="indefinite" />
				</path>
				</svg>
			</div>
		</div>
		<div id="context-menu">	
			<p id="context-menu-name"></p>
			<button id="context-menu-delete">Delete Item</button>
			<!--
			<p id="context-menu-size">Adjust Size</p>
			<div>
				<p>Width</p>
				<input type="number" id="item-width">
			</div>
			<div>
				<p>Depth</p>
				<input type="number" id="item-depth">
			</div>
			<div>
				<p>Height</p>
				<input type="number" id="item-height">
			</div>
			-->
			<div>
				<p>Lock in place</p>
				<input type="checkbox" id="fixed"/>
			</div>
			<p key="" id="editor-measurement-type"></p>
		</div>
		<div id="floorTexturesDiv">
			<p class="texture-title">Adjust Floor</p>
			<div class="texture-flex">
				<div>
					<a href="#" class="thumbnail texture-select-thumbnail" texture-url="rooms/textures/light_fine_wood.jpg" texture-stretch="false" texture-scale="300">
						<img data-lazy="rooms/textures/light_fine_wood.jpg" />
					</a>
				</div>
			</div>
		</div>
		<div id="wallTextures">
			<p class="texture-title">Adjust Wall</p>
			<div class="texture-flex">
				<div>
					<a href="#" class="thumbnail texture-select-thumbnail" texture-url="rooms/textures/marbletiles.jpg" texture-stretch="false" texture-scale="300">
						<img data-lazy="rooms/textures/marbletiles.jpg"/>
					</a>
				</div>
				<div>
					<a href="#" class="thumbnail texture-select-thumbnail" texture-url="rooms/textures/wallmap_yellow.png" texture-stretch="true" texture-scale="">
						<img data-lazy="rooms/textures/wallmap_yellow.png"/>
					</a>
				</div>
				<div>
					<a href="#" class="thumbnail texture-select-thumbnail" texture-url="rooms/textures/light_brick.jpg" texture-stretch="false" texture-scale="100">
						<img data-lazy="rooms/textures/light_brick.jpg"/>
					</a>
				</div>
			</div>
		</div>		
		<div class="editor-inventory fadein hidden" id="editor-inventory">
			<div class="editor-inventory-banner">
				<p class="editor-inventory-title" key="editor-inventory">Inventory</p>
				<a href="javascript:void(0);" id="editor-inventory-cross"/>
					<svg version="1.1" version="1.1" viewBox="0 0 512 512" xml:space="preserve" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g class="st2" id="cross"><g class="st0"><line class="st1" x1="112.5" x2="401" y1="112.5" y2="401"/><line class="st1" x1="401" x2="112.5" y1="112.5" y2="401"/></g></g><g id="cross_copy"><path d="M268.064,256.75l138.593-138.593c3.124-3.124,3.124-8.189,0-11.313c-3.125-3.124-8.189-3.124-11.314,0L256.75,245.436   L118.157,106.843c-3.124-3.124-8.189-3.124-11.313,0c-3.125,3.124-3.125,8.189,0,11.313L245.436,256.75L106.843,395.343   c-3.125,3.125-3.125,8.189,0,11.314c1.562,1.562,3.609,2.343,5.657,2.343s4.095-0.781,5.657-2.343L256.75,268.064l138.593,138.593   c1.563,1.562,3.609,2.343,5.657,2.343s4.095-0.781,5.657-2.343c3.124-3.125,3.124-8.189,0-11.314L268.064,256.75z"/></g></svg>
				</a>
			</div>
			<div class="editor-inventory-flex">
				<div class="editor-inventory-row">
					<div class="editor-inventory-row-content" type="furniture">
						<div id="inventory-furniture">
							<p>Furniture</p>
						</div>
						<img data-lazy="/images/furniture.png">
					</div>
				</div>
				<div class="editor-inventory-row">
					<div class="editor-inventory-row-content" type="decoration">
						<div id="inventory-decoration">
							<p>Decoration</p>
						</div>
						<img data-lazy="/images/decoration.png">
					</div>
					<div class="editor-inventory-row-content" type="lights">
						<div id="inventory-lights">
							<p>Lights</p>
						</div>
						<img data-lazy="/images/lights.png">
					</div>
				</div>
				<div class="editor-inventory-row">
					<div class="editor-inventory-row-content" type="windows">
						<div id="inventory-plants">
							<p>Windows</p>
						</div>
						<img data-lazy="/images/windows.png">
					</div>
					<div class="editor-inventory-row-content" type="doors">
						<div id="inventory-others">
							<p>Doors</p>
						</div>
						<img data-lazy="/images/doors.png">
					</div>
				</div>
				<div class="editor-inventory-browser fadein hidden" id="editor-inventory-browser">
					<div class="editor-inventory-banner">
						<p class="editor-inventory-title" id="editor-inventory-browser-title"></p>
						<a href="javascript:void(0);" id="editor-inventory-browser-cross"/>
							<svg version="1.1" version="1.1" viewBox="0 0 512 512" xml:space="preserve" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g class="st2" id="cross"><g class="st0"><line class="st1" x1="112.5" x2="401" y1="112.5" y2="401"/><line class="st1" x1="401" x2="112.5" y1="112.5" y2="401"/></g></g><g id="cross_copy"><path d="M268.064,256.75l138.593-138.593c3.124-3.124,3.124-8.189,0-11.313c-3.125-3.124-8.189-3.124-11.314,0L256.75,245.436   L118.157,106.843c-3.124-3.124-8.189-3.124-11.313,0c-3.125,3.124-3.125,8.189,0,11.313L245.436,256.75L106.843,395.343   c-3.125,3.125-3.125,8.189,0,11.314c1.562,1.562,3.609,2.343,5.657,2.343s4.095-0.781,5.657-2.343L256.75,268.064l138.593,138.593   c1.563,1.562,3.609,2.343,5.657,2.343s4.095-0.781,5.657-2.343c3.124-3.125,3.124-8.189,0-11.314L268.064,256.75z"/></g></svg>
						</a>
					</div>
					<div class="editor-inventory-browser-each" id="browser-furniture">
						<div class="editor-inventory-browser-filter">
							<p class="editor-inventory-browser-filter-title">Filters</p>
							<div class="editor-inventory-browser-filter-scroll">
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Price</p>
									<div class="filter-price">
										<div class="filter-price-range"></div>
										<input type="text" class="filter-price-range-input" readonly>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Manufacturer</p>
									<div class="filter-manufacturer" id="filter-manufacturer-furniture">
										<p brand="all" class="filter-manufacturer-active">All</p>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Type</p>
									<div class="filter-manufacturer" id="filter-type-furniture">
										<p type="all" class="filter-manufacturer-active">All</p>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Color</p>
									<div class="filter-manufacturer" id="filter-color-furniture">
										<p color="all" class="filter-manufacturer-active">All</p>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<div class="filter-button" id="filter-button-furniture">
										<p>Filter</p>
									</div>
								</div>
							</div>
						</div>
						<div class="editor-inventory-browser-results">
							<div class="editor-inventory-browser-searchbar">
								<i class="fa-solid fa-magnifying-glass"></i>
								<input type="text" id="searchbar" class="searchbar" name="searchbar" placeholder="Search" value="">
							</div>
							<div class="editor-inventory-browser-results-scroll" id="add-items">
								<% for (var i = 0; i < objectslength; i++) { %>
									<% if (objects[i].category === 1) { %>
										<div class="editor-inventory-browser-results-each add-item" id="items-wrapper" price="<%= objects[i].price %>" brand="<%= objects[i].brand %>" height="<%= objects[i].height %>" width="<%= objects[i].width %>" depth="<%= objects[i].depth %>" model-name="<%= objects[i].name %>" model-url="<%= objects[i].url %>" model-type="<%= objects[i].type %>">
											<div>
												<p class="editor-inventory-browser-results-each-name">
													<% if (objects[i].brand !== null && objects[i].brand !== "" && objects[i].brand !== undefined) { %>
														<% if (objects[i].price === "" || objects[i].price === null || objects[i].price === undefined || objects[i].price === 0) { %>
															<%= objects[i].brand %> - <%= objects[i].name %>
														<% } else { %>
															<%= objects[i].brand %> - <%= objects[i].name %> (<%= objects[i].price %>$)
														<% } %>
													<% } else { %>
														<% if (objects[i].price === "" || objects[i].price === null || objects[i].price === undefined || objects[i].price === 0) { %>
															<%= objects[i].name %>
														<% } else { %>
															<%= objects[i].name %> (<%= objects[i].price %>$)
														<% } %>
													<% } %>
												</p>
												<table>
													<tr>
														<td>Height</td>
														<td>Width</td>
														<td>Depth</td>
													</tr>
													<tr>
														<td><%= objects[i].height %></td>
														<td><%= objects[i].width %></td>
														<td><%= objects[i].depth %></td>
													</tr>
												</table>
											</div>
											<img data-lazy="<%= objects[i].image %>">
										</div>
									<% } %>
								<% } %>
							</div>
						</div>
					</div>
					<div class="editor-inventory-browser-each" id="browser-decoration">
						<div class="editor-inventory-browser-filter">
							<p class="editor-inventory-browser-filter-title">Filters</p>
							<div class="editor-inventory-browser-filter-scroll">
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Price</p>
									<div class="filter-price">
										<div class="filter-price-range"></div>
										<input type="text" class="filter-price-range-input" readonly>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Manufacturer</p>
									<div class="filter-manufacturer" id="filter-manufacturer-decoration">
										<p brand="all" class="filter-manufacturer-active">All</p>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Type</p>
									<div class="filter-manufacturer" id="filter-type-decoration">
										<p type="all" class="filter-manufacturer-active">All</p>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<p class="editor-inventory-browser-filter-each-title">Color</p>
									<div class="filter-manufacturer" id="filter-color-decoration">
										<p color="all" class="filter-manufacturer-active">All</p>
									</div>
								</div>
								<div class="editor-inventory-browser-filter-each">
									<div class="filter-button" id="filter-button-decoration">
										<p>Filter</p>
									</div>
								</div>
							</div>
						</div>
						<div class="editor-inventory-browser-results">
							<div class="editor-inventory-browser-searchbar">
								<i class="fa-solid fa-magnifying-glass"></i>
								<input type="text" id="searchbar" class="searchbar" name="searchbar" placeholder="Search" value="">
							</div>
							<div class="editor-inventory-browser-results-scroll" id="add-items">
								<% for (var i = 0; i < objectslength; i++) { %>
									<% if (objects[i].category === 2) { %>
										<div class="editor-inventory-browser-results-each add-item" id="items-wrapper" price="<%= objects[i].price %>" brand="<%= objects[i].brand %>" height="<%= objects[i].height %>" width="<%= objects[i].width %>" depth="<%= objects[i].depth %>" model-name="<%= objects[i].name %>" model-url="<%= objects[i].url %>" model-type="<%= objects[i].type %>">
											<div>
												<p class="editor-inventory-browser-results-each-name">
													<% if (objects[i].brand !== null && objects[i].brand !== "" && objects[i].brand !== undefined) { %>
														<% if (objects[i].price === "" || objects[i].price === null || objects[i].price === undefined || objects[i].price === 0) { %>
															<%= objects[i].brand %> - <%= objects[i].name %>
														<% } else { %>
															<%= objects[i].brand %> - <%= objects[i].name %> (<%= objects[i].price %>$)
														<% } %>
													<% } else{ %>
														<% if (objects[i].price === "" || objects[i].price === null || objects[i].price === undefined || objects[i].price === 0) { %>
															<%= objects[i].name %>
														<% } else { %>
															<%= objects[i].name %> (<%= objects[i].price %>$)
														<% } %>
													<% } %>
												</p>
												<table>
													<tr>
														<td>Height</td>
														<td>Width</td>
														<td>Depth</td>
													</tr>
													<tr>
														<td><%= objects[i].height %></td>
														<td><%= objects[i].width %></td>
														<td><%= objects[i].depth %></td>
													</tr>
